{
  "name": "an-offline-life",
  "version": "0.1.0",
  "description": "js13kgames contribution in 2018",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "check-filesize": "node scripts/check-filesize.js",
    "coverage": "nyc report",
    "doc": "jsdoc -d ./docs -p ./package.json -R README.md src",
    "flow:check": "flow check",
    "flow:jsdoc": "flow-jsdoc -d ./src -o ./annotated -c",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "csscomb src/*.css",
    "lint:js": "standard src/*.js",
    "postbuild": "npm run render",
    "postrender": "npm run check-filesize",
    "prebuild": "rm -rf dist/",
    "prestart": "npm run build",
    "publish": "gh-pages -d dist",
    "render": "node ./scripts/inline-html.js",
    "start": "http-server -a 127.0.0.1 -p 8080 dist",
    "test": "nyc mocha",
    "typing": "npm run flow:jsdoc && npm run flow:check"
  },
  "keywords": [
    "js13kgames"
  ],
  "author": "",
  "license": "GPL-3.0+",
  "devDependencies": {
    "chai": "4.1.2",
    "csscomb": "4.2.0",
    "flow-bin": "0.79.1",
    "flow-jsdoc": "0.3.1",
    "gh-pages": "1.2.0",
    "html-minifier": "3.5.20",
    "http-server": "0.11.1",
    "jsdoc": "3.5.5",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "pug": "2.0.3",
    "rollup": "0.65.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-copy": "0.2.3",
    "rollup-plugin-license": "0.7.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "2.0.2",
    "sinon": "6.1.5",
    "sinon-chai": "3.2.0",
    "standard": "12.0.1",
    "zip-dir": "1.0.2"
  },
  "dependencies": {
    "kontra": "4.0.1",
    "mastermind-game": "1.1.2",
    "nation": "1.3.0"
  }
}
